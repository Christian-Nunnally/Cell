<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:viewModel="clr-namespace:Cell.ViewModel.Cells.Types">
    <DataTemplate DataType="{x:Type viewModel:GraphCellViewModel}">
        <Polyline
            Points="{Binding DataPoints}"
            Stroke="{Binding ForegroundColor, Converter={StaticResource ThemeColorConverter}}"
            StrokeEndLineCap="Round"
            StrokeMiterLimit="0"
            StrokeStartLineCap="Round"
            StrokeThickness="2" />
    </DataTemplate>
</ResourceDictionary>